<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/WeatherFragment">

    <fragment
        android:id="@+id/WeatherFragment"
        android:name="com.khmaies.psaweathertest.ui.fragment.WeatherFragment"
        android:label="@string/weather_fragment_label"
        tools:layout="@layout/fragment_weather">

        <action
            android:id="@+id/action_WeatherFragment_to_AddCityFragment"
            app:destination="@id/AddCityFragment" />
    </fragment>
    <fragment
        android:id="@+id/AddCityFragment"
        android:name="com.khmaies.psaweathertest.ui.fragment.AddCityFragment"
        android:label="@string/search_city_weather"
        tools:layout="@layout/fragment_add_city">

    </fragment>
</navigation>